spawn(function() 
    while getgenv().Pickup do
        local player = game:GetService("Players").LocalPlayer
        local remoteEvent = game:GetService("ReplicatedStorage").ReplicaRemoteEvents.Replica_ReplicaSignal

        if remoteEvent then
            for i = 1, 10 do
                remoteEvent:FireServer(i, "collectMoney", player)
            end
        else
            warn("Remote event not found!")
        end

        task.wait(0.5) 
    end

end)
